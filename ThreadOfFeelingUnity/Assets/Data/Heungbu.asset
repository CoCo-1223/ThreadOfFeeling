  storyTitle: "\uD765\uBD80\uC640 \uB180\uBD80"
  storyCoverImage: {fileID: 21300000, guid: 13edc1af142814c11a5d38667098eafe, type: 3}
  storyDescription: "\uB180\uBD80 \uB54C\uBB38\uC5D0 \uC18D\uC0C1\uD558\uACE0 \uC544\uD320\uB358
    \uC77C\uB4E4\uC774 \uC788\uC5C8\uC9C0\uB9CC, \uC120\uC0DD\uB2D8 \uCE6D\uCC2C\uACFC
    \uC2E0\uB098\uB294 \uC120\uBB3C\uC744 \uBC1B\uACE0, \uB9C8\uC9C0\uB9C9\uC5D4
    \uB180\uBD80\uC640 \uB2E4\uC2DC \uCE5C\uD558\uAC8C \uD654\uD574\uD558\uB294 \uC774\uC57C\uAE30\uC608\uC694!"
  storyTag: "#10\uAC1C\uC52C #\uAC01 30\uBB38\uC81C #\uBC15"
  storyReward: {fileID: 11400000, guid: e112512e30ebb0d4f92321050ec93a95, type: 2}
  scenarios:
  - image: {fileID: 477351349437573007, guid: 2808adf8cf96b42b58400b5174ae5195, type: 3}
    dialogueText: "\uB180\uBD80\uAC00 \uC7AC\uBBF8\uC788\uB294 \uBE14\uB85D \uC313\uAE30
      \uB180\uC774\uB97C \uD558\uACE0 \uC788\uC2B5\uB2C8\uB2E4. \uB098\uB3C4 \uAC19\uC774
      \uB180\uACE0 \uC2F6\uC740\uB370, \uB180\uBD80\uAC00 \"\uB10C \uC548\uB3FC,
      \uC800\uB9AC \uAC00!\"\uB77C\uACE0 \uB9D0\uD569\uB2C8\uB2E4."
    ttsClip: {fileID: 8300000, guid: 928f4ded99796ee428f471eaa3b05974, type: 3}
    quizzes:
    - questionText: "\uB180\uBD80\uAC00 \uAC19\uC774 \uB180\uC544\uC8FC\uC9C0 \uC54A\uACE0
        \"\uC800\uB9AC \uAC00!\"\uB77C\uACE0 \uB9D0\uD588\uC5B4\uC694. \uC9C0\uAE08
        \uB0B4 \uB9C8\uC74C\uC740 \uC5B4\uB5A4\uAC00\uC694?"
      questionVoice: {fileID: 8300000, guid: b82f86aa6430d9449b645f1dbcf81995, type: 3}
      answer1: "\uC11C\uC6B4\uD574\uC694"
      answer2: "\uC2E0\uB098\uC694"
      correctAnswerIndex: 0
      correctFeedback: "\uB9DE\uC558\uC5B4\uC694! \uAC19\uC774 \uB180\uACE0 \uC2F6\uC5C8\uB294\uB370
        \uAC70\uC808\uB2F9\uD558\uBA74 \uC815\uB9D0 \uB9C8\uC74C\uC774 \uC18D\uC0C1\uD558\uACE0
        \uC11C\uC6B4\uD558\uC8E0. \uADF8 \uC0C1\uD669\uC5D0\uC11C \uB290\uAEF4\uC9C0\uB294
        \uAC10\uC815\uC744 \uC544\uC8FC \uC798 \uC774\uD574\uD558\uACE0 \uC788\uB124\uC694!"
      wrongFeedback: "\uB2E4\uC2DC \uD55C\uBC88 \uC0DD\uAC01\uD574 \uBCFC\uAE4C\uC694?
        \uB180\uBD80\uAC00 '\uC800\uB9AC \uAC00!'\uB77C\uACE0 \uB9D0\uD588\uC744
        \uB54C, \uAE30\uC058\uAC70\uB098 \uC7AC\uBBF8\uC788\uC9C0\uB294 \uC54A\uC558\uC744
        \uAC70\uC608\uC694. \uAC19\uC774 \uB180\uACE0 \uC2F6\uC740 \uB9C8\uC74C\uC774
        \uCEF8\uAE30 \uB54C\uBB38\uC5D0 \uC18D\uC0C1\uD558\uACE0 \uC11C\uC6B4\uD55C
        \uB9C8\uC74C\uC774 \uB4DC\uB294 \uAC8C \uB2F9\uC5F0\uD558\uB2F5\uB2C8\uB2E4."
      targetType: 0
      isCommon: 0
    - questionText: "\uC774\uB7F4 \uB54C \uB098\uB294 \uC5B4\uB5BB\uAC8C \uB9D0\uD558\uB294
        \uAC8C \uC88B\uC744\uAE4C\uC694?"
      questionVoice: {fileID: 0}
      answer1: "\uB098\uB3C4 \uAC19\uC774 \uB180\uACE0 \uC2F6\uC5B4."
      answer2: "\uB180\uBD80\uB97C \uB611\uAC19\uC774 \uBC00\uCCD0\uC694."
      correctAnswerIndex: 0
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
      targetType: 1
      isCommon: 0
    - questionText: "\uADF8\uB54C \uB2E4\uB978 \uCE5C\uAD6C\uAC00 \uC640\uC11C \"\uAC19\uC774
        \uB180\uC790!\"\uB77C\uACE0 \uB9D0\uD574\uC92C\uC5B4\uC694. \uC774\uC81C
        \uB0B4 \uAE30\uBD84\uC740 \uC5B4\uB5A4\uAC00\uC694?"
      questionVoice: {fileID: 0}
      answer1: "\uC18D\uC0C1\uD574\uC694."
      answer2: "\uAE30\uBD84\uC774 \uD480\uB838\uC5B4\uC694."
      correctAnswerIndex: 0
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
      targetType: 2
      isCommon: 0
  - image: {fileID: 8915712362660643056, guid: 6e79c46dc71754886bbd9c0d01170a67, type: 3}
    dialogueText: "\uD615 \uB180\uBD80\uAC00 \uB9DB\uC788\uB294 \uACFC\uC790\uB97C
      \uD63C\uC790 \uBA39\uACE0 \uC788\uC2B5\uB2C8\uB2E4. \uB3D9\uC0DD \uD765\uBD80\uAC00
      \"\uB098\uB3C4 \uD558\uB098\uB9CC \uC918\"\uB77C\uACE0 \uBD80\uD0C1\uD588\uC9C0\uB9CC,
      \uB180\uBD80\uB294 \"\uC2EB\uC5B4, \uC774\uAC74 \uB0B4 \uAC70\uC57C!\"\uB77C\uACE0
      \uC18C\uB9AC\uCE69\uB2C8\uB2E4."
    ttsClip: {fileID: 8300000, guid: 7ca538145813797439a9cd404a7d0730, type: 3}
    quizzes:
    - questionText: "\uD615 \uB180\uBD80\uAC00 \uACFC\uC790\uB97C \uB098\uB220\uC8FC\uC9C0
        \uC54A\uC544\uC11C \uD765\uBD80\uB294 \uC815\uB9D0 \uC11C\uC6B4\uD574\uC694.
        \uC9C0\uAE08 \uD765\uBD80\uC758 \uAE30\uBD84\uC740 \uC5B4\uB5A4\uAC00\uC694?"
      questionVoice: {fileID: 8300000, guid: 2c408450f76fe6740b5633ec37fb7b85, type: 3}
      answer1: "\uC2AC\uD37C\uC694"
      answer2: "\uC990\uAC70\uC6CC\uC694"
      correctAnswerIndex: 0
      correctFeedback: "\uC815\uB2F5! \uC815\uB9D0 \uC798 \uB9DE\uCDC4\uC5B4\uC694!
        \uD765\uBD80\uB294 \uC6A9\uAE30\uB97C \uB0B4\uC11C \uC870\uC2EC\uC2A4\uB7FD\uAC8C
        \uBD80\uD0C1\uD588\uB294\uB370, \uD615\uC774 \uC18C\uB9AC\uB97C \uC9C0\uB974\uBA70
        \uAC70\uC808\uD588\uC73C\uB2C8 \uC5BC\uB9C8\uB098 \uC11C\uC6B4\uD558\uACE0
        \uC2AC\uD390\uC744\uAE4C\uC694? \uC6B0\uB9AC \uCE5C\uAD6C\uB294 \uD765\uBD80\uC758
        \uC18D\uB9C8\uC74C\uC744 \uC815\uB9D0 \uC798 \uD5E4\uC544\uB9AC\uB294 \uCC29\uD55C
        \uB9C8\uC74C\uC744 \uAC00\uC84C\uB124\uC694!"
      wrongFeedback: "\uB2E4\uC2DC \uD55C\uBC88 \uD765\uBD80\uC758 \uB9C8\uC74C\uC744
        \uC0B4\uD3B4\uBCFC\uAE4C\uC694? \uACFC\uC790\uB97C \uBA39\uACE0 \uC2F6\uC5C8\uB294\uB370
        \uD615\uC774 \uC548 \uC8FC\uACE0 '\uB0B4 \uAC70\uC57C!'\uB77C\uACE0 \uC18C\uB9AC\uCCE4\uC744
        \uB54C, \uD765\uBD80\uC758 \uB9C8\uC74C\uC740 \uC11C\uC6B4\uD558\uACE0 \uC18D\uC0C1\uD574\uC11C
        \uB208\uBB3C\uC774 \uB0A0 \uAC83 \uAC19\uC558\uC744 \uAC70\uC608\uC694. \uAE30\uC060
        \uB9AC\uAC00 \uC5C6\uACA0\uC8E0? \uB2E4\uC74C\uC5D4 \uD765\uBD80\uC758 \uB9C8\uC74C\uC744
        \uAF2D \uC548\uC544\uC8FC\uB4EF\uC774 \uC0DD\uAC01\uD574 \uBD10\uC694!"
      targetType: 0
      isCommon: 0
    - questionText: "\uB108\uBB34 \uC18D\uC0C1\uD560 \uB54C, \uD765\uBD80\uB294 \uC5B4\uB5BB\uAC8C
        \uD558\uB294 \uAC8C \uC88B\uC744\uAE4C\uC694?"
      questionVoice: {fileID: 0}
      answer1: "\uB180\uBD80\uB97C \uB54C\uB824\uC694."
      answer2: "\uC5C4\uB9C8\uC5D0\uAC8C \uC18D\uC0C1\uD55C \uB9C8\uC74C\uC744 \uB9D0\uD574\uC694."
      correctAnswerIndex: 1
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
      targetType: 1
      isCommon: 0
    - questionText: "\uC7A0\uC2DC \uD6C4, \uC5C4\uB9C8\uAC00 \uD765\uBD80\uC5D0\uAC8C
        \uB2E4\uB978 \uB9DB\uC788\uB294 \uBE75\uC744 \uC8FC\uC168\uC5B4\uC694. \uC774\uC81C
        \uD765\uBD80\uC758 \uAE30\uBD84\uC740 \uC5B4\uB5A4\uAC00\uC694?"
      questionVoice: {fileID: 0}
      answer1: "\uAE30\uBD84\uC774 \uC88B\uC544\uC84C\uC5B4\uC694."
      answer2: "\uACC4\uC18D \uD654\uAC00 \uB098\uC694."
      correctAnswerIndex: 0
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
      targetType: 2
      isCommon: 0
  - image: {fileID: 6282487003229605118, guid: fcd28f6385a7d4241bc7657a6b7faa84, type: 3}
    dialogueText: "\uB180\uC774\uD130\uC5D0\uC11C \uD765\uBD80\uAC00 \uB6F0\uC5B4\uAC00\uB2E4\uAC00
      \uB118\uC5B4\uC838\uC11C \uBB34\uB98E\uC744 \uB2E4\uCCE4\uC2B5\uB2C8\uB2E4.
      \uD765\uBD80\uAC00 \"\uC544\uC57C!\"\uD558\uBA70 \uC6B8\uACE0 \uC788\uC2B5\uB2C8\uB2E4."
    ttsClip: {fileID: 8300000, guid: 27b8882542e42224292894aa47bb0c36, type: 3}
    quizzes:
    - questionText: "\uD765\uBD80\uAC00 \uB118\uC5B4\uC838\uC11C \uC6B8\uACE0 \uC788\uC5B4\uC694.
        \uC9C0\uAE08 \uD765\uBD80\uC758 \uB9C8\uC74C\uC740 \uC5B4\uB5A8\uAE4C\uC694?"
      questionVoice: {fileID: 8300000, guid: c5138a14fd27bfb4a806f620d5c9bbed, type: 3}
      answer1: "\uC544\uD504\uACE0 \uC18D\uC0C1\uD574\uC694"
      answer2: "\uC7AC\uBBF8\uC788\uC5B4\uC694"
      correctAnswerIndex: 0
      correctFeedback: "\uB9DE\uC544\uC694! \uC815\uB9D0 \uC798 \uC54C\uC558\uB124\uC694!
        \uB118\uC5B4\uC9C8 \uB54C \uC5BC\uB9C8\uB098 \uC544\uD504\uACE0 \uB180\uB790\uC744\uAE4C\uC694?
        \uC544\uD508\uB370\uB2E4 \uB208\uBB3C\uAE4C\uC9C0 \uB098\uC11C \uB354 \uC11C\uB7FD\uACE0
        \uC2AC\uD390\uC744 \uAC70\uC608\uC694. \uC6B0\uB9AC \uCE5C\uAD6C\uCC98\uB7FC
        \uD765\uBD80\uC758 \uC544\uD508 \uB9C8\uC74C\uC744 \uD5E4\uC544\uB824\uC8FC\uB294
        \uB530\uB73B\uD55C \uCE5C\uAD6C\uAC00 \uC788\uC73C\uBA74 \uC88B\uACA0\uC5B4\uC694!"
      wrongFeedback: "\uC7A0\uAE50, \uB2E4\uC2DC \uD55C\uBC88 \uC0DD\uAC01\uD574
        \uBCFC\uAE4C\uC694? \uC544\uD30C\uC11C \uC6B8\uACE0 \uC788\uB294 \uCE5C\uAD6C\uAC00
        \uC7AC\uBC0C\uAC70\uB098 \uAD1C\uCC2E\uC744\uAE4C\uC694? \uBB34\uB98E\uC774
        \uB108\uBB34 \uC544\uD30C\uC11C \uC2AC\uD514\uACFC \uB180\uB780 \uB9C8\uC74C\uC774
        \uCEE4\uC84C\uC744 \uAC70\uC608\uC694. \uB2E4\uC74C\uC5D4 \uC544\uD508 \uCE5C\uAD6C\uB97C
        \uBCF4\uB294 \uAC83\uCC98\uB7FC \uC870\uC6A9\uD788 \uB9C8\uC74C\uC744 \uB290\uAEF4\uBD10\uC694!"
      targetType: 0
      isCommon: 0
    - questionText: "\uC6B8\uACE0 \uC788\uB294 \uD765\uBD80\uB97C \uBD24\uC744 \uB54C,
        \uB098\uB294 \uC5B4\uB5BB\uAC8C \uD574\uC57C \uD560\uAE4C\uC694?"
      questionVoice: {fileID: 0}
      answer1: "\"\uAD1C\uCC2E\uC544?\"\uB77C\uACE0 \uBB3C\uC5B4\uBCF4\uACE0 \uC120\uC0DD\uB2D8\uAED8
        \uB2EC\uB824\uAC00\uC694"
      answer2: "\uBABB \uBCF8 \uCC99\uD558\uACE0 \uADF8\uB0E5 \uC9C0\uB098\uAC00\uC694"
      correctAnswerIndex: 0
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
      targetType: 1
      isCommon: 0
    - questionText: "\uB0B4\uAC00 \uB3C4\uC640\uC918\uC11C \uD765\uBD80\uAC00 \"\uACE0\uB9C8\uC6CC\"\uB77C\uACE0
        \uB9D0\uD588\uC5B4\uC694. \uB0B4 \uB9C8\uC74C\uC740 \uC5B4\uB5A4\uAC00\uC694?"
      questionVoice: {fileID: 0}
      answer1: "\uAE30\uBD84\uC774 \uC88B\uACE0 \uBFCC\uB4EF\uD574\uC694."
      answer2: "\uC2EC\uC2EC\uD574\uC694."
      correctAnswerIndex: 0
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
      targetType: 2
      isCommon: 0
  - image: {fileID: 6224017020284958678, guid: 9eee9bc36d9b046efa266872bc09df52, type: 3}
    dialogueText: "\uC720\uCE58\uC6D0\uC5D0\uC11C \uADF8\uB9BC\uC744 \uC5F4\uC2EC\uD788
      \uADF8\uB838\uB354\uB2C8, \uC120\uC0DD\uB2D8\uAED8\uC11C \"\uC815\uB9D0 \uC798
      \uADF8\uB838\uAD6C\uB098!\" \uD558\uC2DC\uBA70 \uBA38\uB9AC \uC704\uC5D0 \uC608\uC05C
      \uBCC4 \uC2A4\uD2F0\uCEE4\uB97C \uBD99\uC5EC\uC8FC\uC168\uC2B5\uB2C8\uB2E4."
    ttsClip: {fileID: 8300000, guid: 6d187d320a6760549bdd825048581899, type: 3}
    quizzes:
    - questionText: "\uC0DD\uAC01\uC9C0\uB3C4 \uBABB\uD588\uB294\uB370 \uC120\uC0DD\uB2D8\uC774
        \uC2A4\uD2F0\uCEE4\uB97C \uC8FC\uC168\uC5B4\uC694. \uC9C0\uAE08 \uB0B4 \uAE30\uBD84\uC740
        \uC5B4\uB5A4\uAC00\uC694?"
      questionVoice: {fileID: 8300000, guid: d40a216e2219d854880fa2d13ed58d5f, type: 3}
      answer1: "\uAE5C\uC9DD \uB180\uB790\uC9C0\uB9CC \uAE30\uBED0\uC694"
      answer2: "\uD654\uAC00 \uB098\uC694"
      correctAnswerIndex: 0
      correctFeedback: "\uB9DE\uC544\uC694! \uC815\uB9D0 \uD589\uBCF5\uD588\uACA0\uC9C0\uC694?
        \uC120\uC0DD\uB2D8\uC758 \uCE6D\uCC2C\uC740 \uC138\uC0C1\uC5D0\uC11C \uAC00\uC7A5
        \uAE30\uC05C \uC120\uBB3C\uC774\uC5D0\uC694! \uC6B0\uB9AC \uCE5C\uAD6C\uAC00
