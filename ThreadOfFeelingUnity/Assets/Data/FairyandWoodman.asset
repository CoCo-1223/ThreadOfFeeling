  scenarios:
  - image: {fileID: -9062015590762506407, guid: d716fd643e1d04dfba6e00808c6d7ade, type: 3}
    dialogueText: "\uB098\uBB34\uAFBC\uC774 \uC0B0\uC5D0\uC11C \uB098\uBB34\uB97C
      \uD558\uACE0 \uC788\uC5B4\uC694. \uAC11\uC790\uAE30 \uC0AC\uC2B4 \uD55C \uB9C8\uB9AC\uAC00
      \uB6F0\uC5B4\uC640\uC694. \"\uB098\uBB34\uAFBC\uB2D8, \uC81C\uBC1C \uC800 \uC880
      \uC0B4\uB824\uC8FC\uC138\uC694!\" \uC0AC\uC2B4\uC740 \uC0AC\uB0E5\uAFBC\uC5D0\uAC8C
      \uCAD3\uAE30\uACE0 \uC788\uC5B4\uC694. \uB5A8\uACE0 \uC788\uB294 \uC0AC\uC2B4\uC774
      \uB108\uBB34 \uBD88\uC30D\uD574 \uBCF4\uC5EC\uC694."
    quizzes:
    - questionText: "\uC0AC\uB0E5\uAFBC\uC5D0\uAC8C \uCAD3\uAE30\uB294 \uC0AC\uC2B4\uC744
        \uBD24\uC5B4\uC694. \uB098\uBB34\uAFBC\uC758 \uC9C0\uAE08 \uAE30\uBD84\uC740?"
      answer1: "\uBD88\uC30D\uD558\uACE0 \uB3C4\uC640\uC8FC\uACE0 \uC2F6\uC5B4\uC694"
      answer2: "\uC2E0\uB098\uC694"
      correctAnswerIndex: 0
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
    - questionText: "\uB204\uAD70\uAC00 \"\uB3C4\uC640\uC8FC\uC138\uC694!\"\uB77C\uACE0
        \uB9D0\uD574\uC694. \uC5B4\uB5BB\uAC8C \uD574\uC57C \uD560\uAE4C\uC694?"
      answer1: "\uC548\uC804\uD55C \uBC29\uBC95\uC73C\uB85C \uB3C4\uC640\uC8FC\uAC70\uB098
        \uC5B4\uB978\uC744 \uBD88\uB7EC\uC694"
      answer2: "\uBAA8\uB978 \uCC99\uD558\uACE0 \uAC00\uC694"
      correctAnswerIndex: 0
      correctFeedback: "\uC798\uD588\uC5B4\uC694"
      wrongFeedback: "\uC544\uC26C\uC6CC\uC694"
    - questionText: "\uB098\uBB34\uAFBC\uC774 \uC0AC\uC2B4\uC744 \uB098\uBB47\uB354\uBBF8
